{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/components/ui/carousel.jsx", "src/app/components/ui/chart.jsx", "src/app/components/ui/checkbox.jsx", "src/app/components/ui/collapsible.jsx", "src/app/components/ui/command.jsx", "src/app/components/ui/dialog.jsx", "src/app/components/ui/drawer.jsx", "src/app/components/ui/dropdown-menu.jsx", "src/app/components/ui/form.jsx", "src/app/components/ui/context-menu.jsx", "src/app/components/figma/ImageWithFallback.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
